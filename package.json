{
  "name": "angular-start",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "wasm": true,
  "jspm": {
    "configFiles": {
      "jspm": "src/jspm.config.js"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^2.4.8",
      "@angular/http": "npm:@angular/http@^2.4.8",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.4.8",
      "@angular/router": "npm:@angular/router@^3.4.8",
      "reflect-metadata": "npm:reflect-metadata@^0.1.10",
      "shim": "npm:shim@^0.6.0"
    },
    "devDependencies": {
      "brorand": "npm:brorand@^1.1.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "elliptic": "npm:elliptic@^6.4.0",
      "hmac-drbg": "npm:hmac-drbg@^1.0.0",
      "minimalistic-crypto-utils": "npm:minimalistic-crypto-utils@^1.0.1",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "plugin-typescript": "npm:plugin-typescript@^6.0.4",
      "readable-stream": "npm:readable-stream@^2.2.3",
      "typescript": "npm:typescript@^2.2.1"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@^2.4.8",
      "@angular/compiler": "npm:@angular/compiler@^2.4.8",
      "@angular/core": "npm:@angular/core@^2.4.8",
      "@angular/platform-browser": "npm:@angular/platform-browser@^2.4.8",
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "rxjs": "npm:rxjs@^5.0.1",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "timers": "npm:jspm-nodelibs-timers@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zone.js": "npm:zone.js@^0.7.2"
    },
    "overrides": {
      "npm:@angular/common@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/common": ".",
          "./testing": "./bundles/common-testing.umd.js"
        }
      },
      "npm:@angular/compiler@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/compiler": ".",
          "./testing": "./bundles/compiler-testing.umd.js"
        }
      },
      "npm:@angular/core@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/core": ".",
          "./testing": "./bundles/core-testing.umd.js"
        }
      },
      "npm:@angular/http@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/http": ".",
          "./testing": "./bundles/http-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser-dynamic@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser-dynamic": ".",
          "./testing": "./bundles/platform-browser-dynamic-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser@2.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser": ".",
          "./testing": "./bundles/platform-browser-testing.umd.js"
        }
      },
      "npm:@angular/router@3.4.8": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/router": ".",
          "./testing": "./bundles/router-testing.umd.js"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:reflect-metadata@0.1.10": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      },
      "npm:typescript@2.2.1": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      }
    }
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.8",
    "chalk": "^1.1.3",
    "connect": "^3.6.0",
    "connect-history-api-fallback": "^1.3.0",
    "cors": "^2.8.1",
    "gulp-typescript": "^3.1.5",
    "helmet": "^3.4.0",
    "moment": "^2.17.1",
    "serve-static": "^1.11.2",
    "typescript": "^2.2.0"
  }
}
